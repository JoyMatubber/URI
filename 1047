#include <iostream>

using namespace std;

int main()
{
   int a,b,c,d,s,s1;
   cin>>a>>b>>c>>d;
   if(a==c && b==d)
   {

    cout<<"O JOGO DUROU 24 HORA(S) E 0 MINUTO(S)"<<endl;

   }
   if(b<d)
   {
       s=c-a;
       s1=d-b;
       cout<<"O JOGO DUROU "<<s<<" HORA(S) E "<<s1<<" MINUTO(S)"<<endl;

   }
   if(b>d)
   {
       s=(c-1)-a;
       s1=(d+60)-b;
       cout<<"O JOGO DUROU "<<s<<" HORA(S) E "<<s1<<" MINUTO(S)"<<endl;

   }


    return 0;
}
