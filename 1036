#include <bits/stdc++.h>

using namespace std;

int main()
{
double A,B,C;
cin>>A>>B>>C;
double w=(pow(B,2)-(4*A*C));


if (w>=0 && A!=0)
{
    double s=sqrt(w);
    double R1=((-B+s)/(2*A));
double R2=((-B-s)/(2*A));

cout<<fixed<<setprecision(5)<<"R1 = "<<R1<<endl;
cout<<fixed<<setprecision(5)<<"R2 = "<<R2<<endl;


}

else


    cout<<"Impossivel calcular"<<endl;





    return 0;
}
