#include <bits/stdc++.h>

using namespace std;

int main()
{

   float a;
   cin>>a;
   if(a>=0 && a<=400.00)
   {
     float m=a*0.15;
       float n=a+m;

       cout<<fixed<<setprecision(2)<<"Novo salario: "<<n<<endl;
       cout<<fixed<<setprecision(2)<<"Reajuste ganho: "<<m<<endl;
       cout<<"Em percentual: 15 %"<<endl;
   }
   if(a>=400.01 &&a<=800.00)
   {
float m=a*0.12;
       float n=a+m;

       cout<<fixed<<setprecision(2)<<"Novo salario: "<<n<<endl;
       cout<<fixed<<setprecision(2)<<"Reajuste ganho: "<<m<<endl;
       cout<<"Em percentual: 12 %"<<endl;
   }
   if(a>=800.01 &&a<=1200.00)
   {

      float m=a*0.10;
       float n=a+m;
       cout<<fixed<<setprecision(2)<<"Novo salario: "<<n<<endl;
       cout<<fixed<<setprecision(2)<<"Reajuste ganho: "<<m<<endl;
       cout<<"Em percentual: 10 %"<<endl;
   }
   if(a>=1200.01 &&a<=2000.00)
   {

      float m=a*0.07;
       float n=a+m;
       cout<<fixed<<setprecision(2)<<"Novo salario: "<<n<<endl;
       cout<<fixed<<setprecision(2)<<"Reajuste ganho: "<<m<<endl;
       cout<<"Em percentual: 7 %"<<endl;
   }
    if(a>2000.00)
   {
float m=a*0.04;
       float n=a+m;
       cout<<fixed<<setprecision(2)<<"Novo salario: "<<n<<endl;
       cout<<fixed<<setprecision(2)<<"Reajuste ganho: "<<m<<endl;
       cout<<"Em percentual: 4 %"<<endl;
   }






    return 0;
}
