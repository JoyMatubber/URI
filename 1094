#include <bits/stdc++.h>

using namespace std;

int main()
{


   int i,n,x;
   char ch;
   int s=0,s1=0,s2=0;
   cin>>n;
   for(i=1;i<=n;i++)
   {
       cin>>x>>ch;

       if(ch=='C')
       {

           s=s+x;
       }
       if(ch=='R')
       {

           s1=s1+x;
       }
       if(ch=='S')
       {

           s2=s2+x;
       }
   }
 int   st=s+s1+s2;
 double p1=(double)(s*100)/st;
  double p2=(double)(s1*100)/st;
   double p3=(double)(s2*100)/st;
 cout<<"Total: "<<st<<" cobaias"<<endl;
 cout<<"Total de coelhos: "<<s<<endl;
 cout<<"Total de ratos: "<<s1<<endl;
 cout<<"Total de sapos: "<<s2<<endl;
 cout<<"Percentual de coelhos: "<<fixed<<setprecision(2)<<p1<<" %"<<endl;
 cout<<"Percentual de ratos: "<<fixed<<setprecision(2)<<p2<<" %"<<endl;
 cout<<"Percentual de sapos: "<<fixed<<setprecision(2)<<p3<<" %"<<endl;

    return 0;
}
