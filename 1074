#include <iostream>

using namespace std;

int main()
{
     int t,n,i;
   cin>>t;
    for(i=1;i<=t;i++){
        cin>>n;
        if(n%2!=0){
            if(n>0){
                cout<<"ODD POSITIVE"<<endl;;
            }
            else if(n<0){
               cout<<"ODD NEGATIVE"<<endl;;
            }
        }
        if(n%2==0){
            if(n>0){
                cout<<"EVEN POSITIVE"<<endl;
            }
            else if(n<0){
               cout<<"EVEN NEGATIVE"<<endl;
            }
        }
        if(n==0){
            cout<<"NULL"<<endl;
        }
    }
    return 0;
}





