#include <iostream>

using namespace std;

int main()
{

    int i=1;
    int s1=0;
    int s2=0;
    int s3=0;
   while(i!=4)
   {
    cin>>i;
       if(i==1)
       {
         s1=s1+1;

       }
       if(i==2)
       {
         s2=s2+1;

       }
       if(i==3)
       {
         s3=s3+1;

       }
   }
cout<<"MUITO OBRIGADO"<<endl;
cout<<"Alcool: "<<s1<<endl;
cout<<"Gasolina: "<<s2<<endl;
cout<<"Diesel: "<<s3<<endl;

    return 0;
}
