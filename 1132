#include <bits/stdc++.h>

using namespace std;

int main()
{
     int a,b;
     cin>>a>>b;
int i;
   int s=0;
   if(b>a){
    for(i=a;i<=b;i++){


          if(i%13!=0)
          {

             s=s+i;

          }

    } cout<<s<<endl;
   }


   if(a>b)
   {
       for(i=b;i<=a;i++)
       {

          if(i%13!=0)
          {

             s=s+i;

          }


       }
       cout<<s<<endl;




   }

    return 0;

}
